RewriteEngine On

####Secure without SSL
#RewriteCond %{HTTP_HOST} ^amjedidiah\.com.ng [NC]
#RewriteCond %{SERVER_PORT} 80
#RewriteRule ^(.*)$ https://www.amjedidiah.com.ng/$1 [R,L]

## Enforce HTTPS
RewriteCond %{HTTPS} off
RewriteRule ^(.*)$ https://%{HTTP_HOST}%{REQUEST_URI} [L,R=301]

###Rewrite Get Query
RewriteRule    ^([A-Za-z0-9-]+)/?$    index.php?p=$1    [NC,L]
RewriteRule    ^([A-Za-z0-9-]+)/?$    index.php?p=$1    [NC,L]
